{{#with footer}}
	<footer>
        <hr class="my-5">
		<div>
			{{#with social}}
				{{> link email}}
				{{> link phone}}
				{{> link linkedin blank=true woLine=true}}
			{{/with}}
		</div>
		<div class="my-3">
			<em>Copyright (c){{../header/name/firstname}} {{../header/name/lastname}}</em>
		</div>
		<div>
			<address>{{../header/address}}</address>
		</div>
    </footer>
{{/with}}