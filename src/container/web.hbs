{{#with web}}
    {{#each languages}}
        <div class="m-2 mb-5">
            {{> title name=name big=true}}
            <u>Version: {{version}}</u><br>
            <em>{{description}}</em>
        </div>
        <div class="card m-2">
            {{#if elements}}                
                {{#each elements}}
                    <div class="card-body"></div>
                        <div class="card-title m-2">
                            {{> element name=name description=description}}
                        </div>
                        <h4 class="mx-2">Attributes:</h4>
                        <ul class="list-group m-2">
                            {{#each attributes}}
                                <li class="list-group-item m-2">
                                    {{> element name=name description=description}}
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                {{/each}}
            {{/if}}
            {{#if properties}}
                {{#each properties}}
                    <div class="card mb-2">
                        {{> element name=name description=description}}
                        <h4>Values:</h4>
                        <ul class="list-group m-2">
                            {{#each values}}
                                <li class="list-group-item m-2">
                                    {{> element name=value description=description}}
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                {{/each}}
            {{/if}}
            {{#if functions}}
                {{#each functions}}
                    <div class="card mb-2">
                        {{> element name=name description=description}}
                        <h4>Parameters:</h4>
                        <ul class="list-group m-2">
                            {{#each parameters}}
                                <li class="list-group-item m-2">
                                    {{> element name=name description=description}}
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                {{/each}}
            {{/if}}
        </div>
    {{/each}}
{{/with}}